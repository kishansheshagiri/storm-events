jQuery(document).ready(function(){
  var batchNo = jQuery('.batch').html();
  jQuery('#event_state_fips').on('change load',function() {
    var url = '/get_counties_dropdown?us_state_id=' + $(this).val()
    $('#event_county_fips').empty()
    $.get(url, function(data) {
      $.each(JSON.parse(data), function(index, item) {
        $('#event_county_fips').append(new Option(item.name, item.fips));
      });
    });
  })
})